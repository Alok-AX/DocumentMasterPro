{
    "version": 2,
    "builds": [
      {
        "src": "server/index.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server/index.ts"
      }
    ],
    
        "functions": {
          "users/**/*.js": {
            "maxDuration": 30
          }
        }
      }
  