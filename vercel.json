{
    "version": 2,
    "buildCommand": "npm run build",
    "installCommand": "npm install",
    "framework": null,
    "functions": {
      "server/index.ts": {
        "memory": 1024,
        "maxDuration": 10
      }
    },
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/server/index.ts"
      }
    ],
    "env": {
      "NODE_ENV": "production",
      "SESSION_SECRET": "your_session_secret"
    }
  }